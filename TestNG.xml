<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" allow-return-values="true" >
    <listeners>
        <listener class-name="utilitis.Listener"/>
    </listeners>
    <test verbose="1" thread-count="1" name="Test" parallel="classes">
        <groups>
            <run>
                <include name="sanity-group"/>
            </run>
        </groups>
       <classes>

           <class name="TestsObjects.CreateCircleTest"/>
           <class name="TestsObjects.CreatePolygonTest"/>
           <class name="TestsObjects.DeleteCircleTest"/>
           <class name="TestsObjects.DivdePolygonTest"/>
           <class name="TestsObjects.EditPolyLineTest"/>
           <class name="TestsObjects.DragPolygonTest"/>
           <class name="TestsObjects.CreatePolyLineTest"/>
           <class name="TestsObjects.ZoomInTest"/>
           <class name="TestsObjects.ZoomOutTest"/>
       </classes>
    </test>
</suite>